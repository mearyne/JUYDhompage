<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
	id="WebApp_ID" version="4.0">
	<display-name>booking</display-name>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.htm</welcome-file>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>default.html</welcome-file>
		<welcome-file>default.htm</welcome-file>
		<welcome-file>default.jsp</welcome-file>
	</welcome-file-list>

	<!-- 로그인.jsp 파일 -->
	<servlet>
		<servlet-name>login</servlet-name>
		<jsp-file>/login/login.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>

	<!-- mypage.jsp 파일 -->
	<servlet>
		<servlet-name>mypage</servlet-name>
		<jsp-file>/mypage/mypage.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>mypage</servlet-name>
		<url-pattern>/mypage</url-pattern>
	</servlet-mapping>

	<!-- favoriteList.jsp 파일 -->
	<servlet>
		<servlet-name>favoriteList</servlet-name>
		<jsp-file>/mypage/favoriteList.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>favoriteList</servlet-name>
		<url-pattern>/favoriteList</url-pattern>
	</servlet-mapping>

	<!-- ordered.jsp 파일 -->
	<servlet>
		<servlet-name>ordered</servlet-name>
		<jsp-file>/mypage/ordered.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>ordered</servlet-name>
		<url-pattern>/ordered</url-pattern>
	</servlet-mapping>


	<!-- review.jsp 파일 -->
	<servlet>
		<servlet-name>review</servlet-name>
		<jsp-file>/mypage/review.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>review</servlet-name>
		<url-pattern>/review</url-pattern>
	</servlet-mapping>


	<!-- shopPage.jsp 파일 -->
	<servlet>
		<servlet-name>shop</servlet-name>
		<jsp-file>/shop/shop.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>shop</servlet-name>
		<url-pattern>/shop</url-pattern>
	</servlet-mapping>


	<!-- index.jsp 파일 -->
	<servlet>
		<servlet-name>index</servlet-name>
		<jsp-file>/index.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>index</servlet-name>
		<url-pattern>/index</url-pattern>
	</servlet-mapping>

	<!-- index.jsp 파일 -->
	<servlet>
		<servlet-name>loginUser</servlet-name>
		<jsp-file>/login/login_user.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>loginUser</servlet-name>
		<url-pattern>/loginUser</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>loginMaster</servlet-name>
		<jsp-file>/login/login_master.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>loginMaster</servlet-name>
		<url-pattern>/loginMaster</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>joinUser</servlet-name>
		<jsp-file>/join/join_user.jsp</jsp-file>
	</servlet>

	<servlet-mapping>
		<servlet-name>joinUser</servlet-name>
		<url-pattern>/joinUser</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>joinMaster</servlet-name>
		<jsp-file>/join/join_master.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>joinMaster</servlet-name>
		<url-pattern>/joinMaster</url-pattern>
	</servlet-mapping>


</web-app>